{
    "name": "mini-rx",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "ng": "nx",
        "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
        "start:angular-demo": "nx build mini-rx-store && nx build mini-rx-store-ng && nx serve mini-rx-angular-demo",
        "dev": "nx serveAppAndWatchLibs",
        "build:mini-rx-angular-demo": "nx build mini-rx-angular-demo",
        "build:mini-rx-store": "nx build mini-rx-store",
        "build:mini-rx-store-ng": "nx build mini-rx-store-ng",
        "build:all": "nx build mini-rx-store && nx build mini-rx-store-ng && nx build mini-rx-angular-demo",
        "test:mini-rx-store": "nx test mini-rx-store",
        "test:mini-rx-store:watch": "nx test mini-rx-store --watch",
        "test:mini-rx-store:coverage": "nx test mini-rx-store --codeCoverage",  
        "test:mini-rx-store-ng": "nx test mini-rx-store-ng",
        "test:mini-rx-store-ng:watch": "nx test mini-rx-store-ng --watch",
        "deploy:docs": "npm run deploy --prefix docs",
        "build:mini-rx-angular-demo:sourcemap": "nx build mini-rx-angular-demo --source-map",
        "build:mini-rx-angular-demo:sourcemap:stats": "npm run build:mini-rx-angular-demo:sourcemap && source-map-explorer dist/apps/mini-rx-angular-demo/**/*.js",
        "prepare": "husky install"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~13.2.0",
        "@angular/common": "~13.2.0",
        "@angular/compiler": "~13.2.0",
        "@angular/core": "~13.2.0",
        "@angular/forms": "~13.2.0",
        "@angular/platform-browser": "~13.2.0",
        "@angular/platform-browser-dynamic": "~13.2.0",
        "@angular/router": "~13.2.0",
        "@nrwl/angular": "15.0.13",
        "angular-in-memory-web-api": "^0.13.0",
        "bootstrap": "^4.4.1",
        "bootstrap-icons": "^1.4.0",
        "lodash-es": "^4.17.21",
        "ngx-toastr": "^14.1.4",
        "rxjs": "~7.4.0",
        "ts-action": "^11.0.0",
        "ts-action-operators": "^9.1.1",
        "tslib": "^2.0.0",
        "zone.js": "~0.11.4"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~13.3.9",
        "@angular-eslint/eslint-plugin": "~13.0.1",
        "@angular-eslint/eslint-plugin-template": "~13.0.1",
        "@angular-eslint/template-parser": "~13.0.1",
        "@angular/cli": "~13.2.0",
        "@angular/compiler-cli": "~13.2.0",
        "@angular/language-service": "~13.2.0",
        "@commitlint/cli": "^16.2.1",
        "@commitlint/config-conventional": "^16.2.1",
        "@jscutlery/semver": "^2.27.2",
        "@nrwl/cli": "13.8.8",
        "@nrwl/cypress": "15.0.13",
        "@nrwl/eslint-plugin-nx": "15.0.13",
        "@nrwl/jest": "13.9.5",
        "@nrwl/js": "15.0.13",
        "@nrwl/linter": "13.9.5",
        "@nrwl/nx-cloud": "13.1.6",
        "@nrwl/web": "15.0.13",
        "@nrwl/workspace": "15.0.13",
        "@types/jest": "27.0.2",
        "@types/lodash-es": "^4.17.6",
        "@types/node": "16.11.7",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "~5.10.0",
        "@typescript-eslint/parser": "~5.10.0",
        "cypress": "^9.1.0",
        "eslint": "~8.7.0",
        "eslint-config-prettier": "8.1.0",
        "eslint-plugin-cypress": "^2.10.3",
        "husky": "^7.0.4",
        "jest": "27.2.3",
        "jest-marbles": "^3.0.2",
        "jest-preset-angular": "11.1.1",
        "lint-staged": "^12.3.7",
        "ng-packagr": "^13.0.0",
        "nx": "13.9.5",
        "postcss": "^8.4.5",
        "postcss-import": "^14.0.2",
        "postcss-preset-env": "^6.7.0",
        "postcss-url": "^10.1.1",
        "prettier": "^2.5.1",
        "source-map-explorer": "^2.5.2",
        "ts-jest": "27.0.5",
        "typescript": "~4.5.2"
    },
    "lint-staged": {
        "*.{js,json,css,scss,md,ts,html}": [
            "prettier --write",
            "git add"
        ],
        "*.format:write": "prettier --write"
    }
}
